		pgsqlPgTsParserPrstoken		INTEGER,
		pgsqlPgTsParserPrsend		INTEGER,
		pgsqlPgTsParserPrsheadline		INTEGER,
		pgsqlPgTsParserPrslextype		INTEGER
	}

