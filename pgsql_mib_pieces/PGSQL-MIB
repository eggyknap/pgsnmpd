		pgsqlPgAuthMembersEntryOID		INTEGER,
		pgsqlPgAuthMembersRoleid		INTEGER,
		pgsqlPgAuthMembersMember		INTEGER,
		pgsqlPgAuthMembersGrantor		INTEGER,
		pgsqlPgAuthMembersAdminOption		TruthValue
	}

